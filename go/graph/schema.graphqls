# GraphQL schema example
#
# https://gqlgen.com/getting-started/

scalar DateTime
scalar Int64
scalar Upload

directive @loggedIn on FIELD_DEFINITION

type Team {
  id: Int64!
  slug: String!
  name: String!
  team_type: String!
  created: DateTime!
}

type Userinfo {
  id: Int64!
  email: String!
  fullName: String!
}

type Query {
  getTeams: [Team!]! @loggedIn
}

type Mutation {
  createTeam: Team! @loggedIn
}

